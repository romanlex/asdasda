{
  "name": "@foxford/market-app",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider razzle start",
    "build": "NODE_OPTIONS=--openssl-legacy-provider razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "commit": "git-cz",
    "lint": "eslint ./src",
    "type-checking": "tsc --rootDir ./src --noEmit --pretty"
  },
  "engines": {
    "node": ">=17.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "@foxford/foxford-js-sdk": "2.1.73",
    "@loadable/component": "5.15.2",
    "@loadable/server": "5.15.2",
    "@sentry/browser": "6.16.0",
    "body-parser": "1.19.0",
    "camelcase": "6.2.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-fetch": "3.1.4",
    "effector": "22.1.2",
    "effector-inspector": "0.7.5",
    "effector-logger": "0.13.1",
    "effector-react": "22.0.6",
    "ejs": "3.1.6",
    "ejs-stream2": "1.0.2",
    "express": "4.17.1",
    "express-promise-router": "4.1.1",
    "express-session": "1.17.2",
    "http-proxy-middleware": "2.0.1",
    "inversify": "6.0.1",
    "js-cookie": "3.0.1",
    "js-logger": "1.6.1",
    "local-storage-fallback": "4.1.2",
    "luxon": "2.1.1",
    "map-obj": "4.3.0",
    "morgan": "1.10.0",
    "npm-upgrade": "3.0.0",
    "patronum": "1.1.0",
    "pretty-error": "4.0.0",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-helmet-async": "1.2.2",
    "react-intl": "5.24.2",
    "react-is": "17.0.2",
    "react-router": "5.2.1",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.3.0",
    "reflect-metadata": "0.1.13",
    "rotating-file-stream": "2.1.6",
    "session-file-store": "1.5.0",
    "styled-components": "5.3.3",
    "styled-normalize": "8.0.7",
    "through": "2.3.8",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "7.14.5",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@foxford/babel-preset-react-app": "1.2.1",
    "@loadable/babel-plugin": "5.13.2",
    "@loadable/webpack-plugin": "5.15.2",
    "@statoscope/webpack-plugin": "5.19.0",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/ejs": "3.1.0",
    "@types/express": "4.17.13",
    "@types/express-promise-router": "3.0.0",
    "@types/express-session": "1.17.4",
    "@types/jest": "27.0.2",
    "@types/js-cookie": "3.0.1",
    "@types/loadable__component": "5.13.4",
    "@types/loadable__server": "5.12.6",
    "@types/luxon": "2.0.7",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.6",
    "@types/ramda": "0.27.60",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/react-helmet": "6.1.4",
    "@types/react-router": "5.1.17",
    "@types/react-router-config": "5.0.3",
    "@types/react-router-dom": "5.3.2",
    "@types/session-file-store": "1.2.2",
    "@types/styled-components": "5.1.15",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "babel-plugin-styled-components": "1.13.3",
    "commitizen": "4.2.4",
    "core-js": "3.19.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-react": "11.0.1",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "npm": "8.1.2",
    "prettier": "2.4.1",
    "razzle": "4.2.5",
    "razzle-plugin-typescript": "4.2.5",
    "ts-jest": "27.0.7",
    "typescript": "4.5.4",
    "webpack-bundle-analyzer": "4.5.0"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
